#include <Arduino.h>
#include "GPS.h"

GPS_UBLOX My_GPS;

void setup()
{
	Serial.begin(9600);			//设置波特率9600和GPS模块一致
        Serial1.begin(9600);			//设置波特率9600和GPS模块一致
	My_GPS.Init_GPS();			//初始化GPS用到的变量
}

void loop()
{
	My_GPS.Get_GPS();
}

